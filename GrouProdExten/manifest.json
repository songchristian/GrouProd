{
	"name": "TabManipulator",
	"manifest_version": 2,
	"version": "1.0",
	"description": "HackDuke",
	"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "buttons.html"
	},
	"permissions": [
	   "tabs","http://*/"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}